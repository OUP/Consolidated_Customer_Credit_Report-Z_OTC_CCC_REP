<core:FragmentDefinition 
    xmlns="sap.m" 
    xmlns:core="sap.ui.core" 
    xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1">
    <!-- Card's Content area code goes here -->

    <List items="{global>/to_C_CUSTOVRDUEBALFINAL/results}">     
        <CustomListItem class="sapUiSmallMargin">
            <!-- name -->
            <HBox class="sapUiTinyMarginBottom">
                <Label text="{@i18n>ODB_NAME}:" class="sapUiTinyMarginEnd"/>
                <Text text="{global>PropertyName}"/>
            </HBox>
            <!-- property name -->
            <HBox class="sapUiTinyMarginBottom">
                <Label text="{@i18n>ODB_PROPERTY_NAME}:" class="sapUiTinyMarginEnd"/>
                <Text text="{global>Amount}"/>
            </HBox>
        </CustomListItem>
    </List>

</core:FragmentDefinition>